// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Survey {
  id           String  @id @default(cuid())
  schema       Json    @default("{}")
  is_published Boolean @default(false)
}
